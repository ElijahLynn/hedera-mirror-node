"""
The query root of the Hedera GraphQL API
"""
type Query {
    account(input: AccountInput!): Account
    block(input: BlockInput!): Block
    contract(input: ContractInput!): Contract
    file(input: FileInput!): File
    network: Network!
    node(id: ID!): Node
    schedule(input: ScheduleInput!): Schedule
    token(input: TokenInput!): Token
    topic(input: TopicInput!): Topic
    transaction(input: TransactionInput!): Transaction
}
