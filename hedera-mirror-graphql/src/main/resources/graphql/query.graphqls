"""
The query root of the Hedera GraphQL API
"""
type Query {
    account(filter: AccountInput!): Account
    block(filter: BlockInput!): Block
    contract(filter: ContractInput!): Contract
    file(filter: FileInput!): File
    network: Network!
    node(id: ID!): Node
    schedule(filter: ScheduleInput!): Schedule
    token(filter: TokenInput!): Token
    topic(filter: TopicInput!): Topic
    transaction(filter: TransactionInput!): Transaction
}
